{
  "expo": {
    "name": "Couaff biz",
    "scheme" : "salonppvendor",
    "slug": "salonAppVendor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "notification":{
      "icon":"./assets/notif.png"
   },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#111111"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.mughees1512.salonAppVendor",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBSw0D88sjoodik8ALNNMhccUL-WQbpwJo"
        }
      },
      "useNextNotificationsApi":true,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-notifications",
        {
          "icon": "./assets/notif.png"
        }
      ]

    ]
  }
}
